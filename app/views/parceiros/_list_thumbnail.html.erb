<% cont = 0 %>
<% parceiros_lista.each do |parceiro| %>

    <% row_open = cont == 0 %>
    <% row_end = cont == 2 %>

    <% if row_open %>
        <div class="row">
    <% end %>

    <%= helper_parceiro_show_thumbnail(parceiro) %>

    <% if row_end %>
        </div>
        <% cont = 0 %>
    <% else %>
        <% cont += 1 %>
    <% end %>

<% end %>